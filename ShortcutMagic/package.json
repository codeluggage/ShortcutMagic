{
  "name": "ShortcutMagic",
  "productName": "ShortcutMagic",
  "description": "ShortcutMagic is a simple tool to see every action in any program, and master them.",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "shortcutmagic.js",
  "repository": "git@github.com:codeluggage/ShortcutMagic.git",
  "scripts": {
    "start": "NODE_ENV=production ./node_modules/.bin/electron shortcutmagic.js",
    "dev": "NODE_ENV=development ./node_modules/.bin/electron shortcutmagic.js",
    "pack-mac": "electron-packager . --asar=false --overwrite --platform=darwin --arch=x64 --icon=assets/wizard.icns --prune=true --osx-sign.identity='Developer ID Application: Matias Forbord (A9ADPQA768)'"
  },
  "build": {
      "appId": "com.thankivity.shortcutmagic",
      "mac": {
          "category": "Productivity"
      }
  },
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "electron-osx-appearance": "^0.1.1",
    "font-awesome": "^4.7.0",
    "nedb": "^1.8.0",
    "nodobjc": "^2.1.0",
    "react": "^15.3.2",
    "react-color": "^2.4.3",
    "react-dom": "^15.3.2",
    "react-sortable-hoc": "^0.1.1",
    "robotjs": "^0.4.5"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.6.5",
    "electron-packager": "^8.6.0",
    "electron-rebuild": "^1.4.0"
  }
}
